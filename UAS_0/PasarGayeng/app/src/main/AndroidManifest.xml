<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="damisbachtiar17.web.id.pasargayeng">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".DaftarTokoActivity"></activity>
        <activity android:name=".DaftarBarangActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".DetailBarang"></activity>
        <activity android:name=".FormPembayaranActivity"></activity>
        <activity android:name=".HalamanPembayaranActivity"></activity>
    </application>

</manifest>